     <!-- http://w3id.org/ro/earth-science#DataResearchObject -->
     
     <owl:Class rdf:about="&easc;DataResearchObject">
      <rdfs:label xml:lang="en">Data-centric Research Object</rdfs:label>
      <rdfs:isDefinedBy rdf:resource="http://w3id.org/ro/earth-science"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ro;ResearchObject"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.openarchives.org/ore/terms/aggregates"/>
                        <owl:someValuesFrom rdf:resource="&wfprov;Artifact"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">A data-centric research object is a research object that aggregates at least one artifact. 
        It might of course aggregates any other kind of resources, but MUST aggregate at least one wfprov:Artifact, 
        such as wf4ever:Dataset, wf4ever:Document, wf4ever:File, wf4ever:Image.
        </rdfs:comment>
    </owl:Class>
    
    <!-- http://w3id.org/ro/earth-science#ServiceResearchObject -->
    
    <owl:Class rdf:about="&easc;ServiceResearchObject">
      <rdfs:label xml:lang="en">Service-centric Research Object</rdfs:label>
      <rdfs:isDefinedBy rdf:resource="http://w3id.org/ro/earth-science"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
