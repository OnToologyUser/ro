    <!-- http://w3id.org/ro/earth-science#centerOf -->

    <owl:ObjectProperty rdf:about="&easc;centerOf">
      <rdfs:isDefinedBy rdf:resource="http://w3id.org/ro/earth-science"/>
        <rdfs:label xml:lang="en">center of</rdfs:label>
        <rdfs:comment xml:lang="en">the center point of the selected feature. Property redefined and adapted from gml v1.0 
        (http://schemas.opengis.net/gml/1.0.0/gml.rdfs) that is not resolvable and incompatible with gml v3 used by geosparql ontology</rdfs:comment>
        <rdfs:range rdf:resource="&sf:Point"/>
        <rdfs:subPropertyOf rdf:resource="&geo;hasGeometry"/>
    </owl:ObjectProperty>
    
